{
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "*.dart": "$(capture).g.dart, $(capture).freezed.dart, $(capture).mocks.dart, $(capture)_none.dart, $(capture)_io.dart, $(capture)_web.dart",
    "pubspec.yaml": "pubspec.lock, pubspec_overrides.yaml",
    "package.json": "bun.lock"
  },
  "dart.flutterTestAdditionalArgs": [
    "--dart-define-from-file=lib/core/config/env/development.env"
  ]
}